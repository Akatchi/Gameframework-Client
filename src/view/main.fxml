<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <left>
      <VBox prefHeight="600.0" prefWidth="163.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="16.0" prefWidth="166.0" text="Playerlist">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <ListView fx:id="playerListView" prefHeight="586.0" prefWidth="163.0" />
            <Button mnemonicParsing="false" onAction="#retrievePlayerList" prefHeight="25.0" prefWidth="164.0" text="Retrieve players" />
         </children>
      </VBox>
   </left>
   <right>
      <VBox prefHeight="600.0" prefWidth="146.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="16.0" prefWidth="166.0" text="Challengelist">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <ListView fx:id="challengeListView" prefHeight="624.0" prefWidth="146.0" />
         </children>
      </VBox>
   </right>
   <center>
      <VBox fx:id="gameBoardBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
